        <span class="c1">// æœ‰å¤šå°‘ä¸ªå…ƒç´ éœ€è¦åç§»</span>
        <span class="kt">int</span> <span class="n">numMoved</span> <span class="o">=</span> <span class="n">len</span> <span class="o">-</span> <span class="n">index</span><span class="o">;</span>
        <span class="c1">// numMovedä¸º0è¡¨ç¤ºæ–°å…ƒç´ æ’å…¥åˆ°æœ€åä¸€ä¸ªç´¢å¼•</span>
        <span class="k">if</span> <span class="o">(</span><span class="n">numMoved</span> <span class="o">==</span> <span class="mi">0</span><span class="o">)</span>
            <span class="c1">// åˆ›å»ºæ–°æ•°ç»„</span>
            <span class="n">newElements</span> <span class="o">=</span> <span class="nc">Arrays</span><span class="o">.</span><span class="na">copyOf</span><span class="o">(</span><span class="n">elements</span><span class="o">,</span> <span class="n">len</span> <span class="o">+</span> <span class="mi">1</span><span class="o">);</span>
