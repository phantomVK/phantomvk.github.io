    <span class="c1">// postWriteRunnableä¸ºç©ºï¼ŒåŒæ­¥æäº¤æ•°æ®åˆ°ç£ç›˜</span>
    <span class="kd">final</span> <span class="kt">boolean</span> <span class="n">isFromSyncCommit</span> <span class="o">=</span> <span class="o">(</span><span class="n">postWriteRunnable</span> <span class="o">==</span> <span class="kc">null</span><span class="o">);</span>

