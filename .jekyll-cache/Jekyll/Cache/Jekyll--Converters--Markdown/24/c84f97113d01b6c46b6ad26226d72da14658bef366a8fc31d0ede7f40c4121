<span class="c1">// æ–‡ä»¶è¢«å¤–éƒ¨æ“ä½œä¿®æ”¹æ—¶æ‰§è¡Œçš„æ–¹æ³•</span>
<span class="kd">private</span> <span class="kt">boolean</span> <span class="nf">hasFileChangedUnexpectedly</span><span class="o">()</span> <span class="o">{</span>
    <span class="kd">synchronized</span> <span class="o">(</span><span class="n">mLock</span><span class="o">)</span> <span class="o">{</span>
        <span class="k">if</span> <span class="o">(</span><span class="n">mDiskWritesInFlight</span> <span class="o">&gt;</span> <span class="mi">0</span><span class="o">)</span> <span class="o">{</span>
            <span class="c1">// å·²çŸ¥æ˜¯æœ¬ç¨‹åºä¸»åŠ¨å¼•èµ·çš„ï¼Œä¸éœ€è¦å¤„ç†</span>
            <span class="k">return</span> <span class="kc">false</span><span class="o">;</span>
        <span class="o">}</span>
    <span class="o">}</span>

    <span class="kd">final</span> <span class="nc">StructStat</span> <span class="n">stat</span><span class="o">;</span>
    <span class="k">try</span> <span class="o">{</span>
        <span class="c1">// Metadata operations don't usually count as a block guard violation</span>
        <span class="c1">// but we explicitly want this one.</span>
        <span class="nc">BlockGuard</span><span class="o">.</span><span class="na">getThreadPolicy</span><span class="o">().</span><span class="na">onReadFromDisk</span><span class="o">();</span>
        <span class="c1">// è¯»å–æ–‡ä»¶çŠ¶æ€</span>
        <span class="n">stat</span> <span class="o">=</span> <span class="nc">Os</span><span class="o">.</span><span class="na">stat</span><span class="o">(</span><span class="n">mFile</span><span class="o">.</span><span class="na">getPath</span><span class="o">());</span>
    <span class="o">}</span> <span class="k">catch</span> <span class="o">(</span><span class="nc">ErrnoException</span> <span class="n">e</span><span class="o">)</span> <span class="o">{</span>
        <span class="k">return</span> <span class="kc">true</span><span class="o">;</span>
    <span class="o">}</span>

    <span class="kd">synchronized</span> <span class="o">(</span><span class="n">mLock</span><span class="o">)</span> <span class="o">{</span>
        <span class="c1">// å†…å­˜ä¸­è®°å½•ç£ç›˜æ–‡ä»¶çš„çŠ¶æ€ï¼Œå’Œå®é™…ç£ç›˜æ–‡ä»¶çš„çŠ¶æ€è¿›è¡Œå¯¹æ¯”</span>
        <span class="k">return</span> <span class="o">!</span><span class="n">stat</span><span class="o">.</span><span class="na">st_mtim</span><span class="o">.</span><span class="na">equals</span><span class="o">(</span><span class="n">mStatTimestamp</span><span class="o">)</span> <span class="o">||</span> <span class="n">mStatSize</span> <span class="o">!=</span> <span class="n">stat</span><span class="o">.</span><span class="na">st_size</span><span class="o">;</span>
