            <span class="nd">@Override</span>
            <span class="kd">public</span> <span class="kt">void</span> <span class="nf">run</span><span class="o">()</span> <span class="o">{</span>
                <span class="c1">// å†™å…¥æ–‡ä»¶ï¼ŒisFromSyncCommitæ˜¯å¦åŒæ­¥å†™å…¥</span>
                <span class="kd">synchronized</span> <span class="o">(</span><span class="n">mWritingToDiskLock</span><span class="o">)</span> <span class="o">{</span>
                    <span class="n">writeToFile</span><span class="o">(</span><span class="n">mcr</span><span class="o">,</span> <span class="n">isFromSyncCommit</span><span class="o">);</span>
