                <span class="kd">synchronized</span> <span class="o">(</span><span class="n">mLock</span><span class="o">)</span> <span class="o">{</span>
                    <span class="n">mDiskWritesInFlight</span><span class="o">--;</span>
                <span class="o">}</span>

                <span class="c1">// ä»…apply()æ—¶æ‰§è¡Œï¼Œè§¦å‘å›è°ƒå‘ŠçŸ¥å†™å…¥å·²å®Œæˆ</span>
                <span class="k">if</span> <span class="o">(</span><span class="n">postWriteRunnable</span> <span class="o">!=</span> <span class="kc">null</span><span class="o">)</span> <span class="o">{</span>
